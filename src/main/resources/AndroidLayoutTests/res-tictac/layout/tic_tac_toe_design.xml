            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_weight="1" android:layout_gravity="center_horizontal">
          <com.android.sdklib.widgets.iOSButton
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:layout_marginTop="10dp"
            android:layout_marginBottom="10dp"
                style="@style/XOSButton"
              android:id="@+id/com.android.sdklib.widgets.iOSButton" android:layout_weight="1" android:background="#ffb0d6ff" xrt:ibaction="touchUpInside-BtnPressedCancel_gameButton6" xrt:iboutlet="gameButton6"/><com.android.sdklib.widgets.iOSButton
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:layout_marginTop="10dp"
            android:layout_marginBottom="10dp"
                style="@style/XOSButton"
              android:id="@+id/com.android.sdklib.widgets.iOSButton" android:layout_weight="1" android:background="#ffb0d6ff" xrt:iboutlet="gameButton7" xrt:ibaction="touchUpInside-BtnPressedCancel_gameButton7"/><com.android.sdklib.widgets.iOSButton
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:layout_marginTop="10dp"
            android:layout_marginBottom="10dp"
                style="@style/XOSButton"
              android:id="@+id/com.android.sdklib.widgets.iOSButton" android:layout_weight="1" android:background="#ffb0d6ff" xrt:ibaction="touchUpInside-BtnPressedCancel_gameButton8" xrt:iboutlet="gameButton8"/></com.android.sdklib.widgets.base_elements.iOSBaseLinearLayout></com.android.sdklib.widgets.base_elements.iOSBaseLinearLayout>